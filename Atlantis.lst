			      C 			MOV EAX, [ESI]
			      C 			GettingSoldierTime EAX,	UserCurrLevel, GetterHandler
			      C 			MOV EAX,	GetterHandler 
			      C 			.IF( Timer_UserBuild == EAX )
			      C 				TriggleBuild 0, [ESI]
			      C 				POPArray User_BuildQueue
			      C 				MOV EAX, User_BuildQueue
			      C 				.IF( EAX != 0 )
			      C 					CopyFrom Timer_UserBuild, 1
			      C 				.ELSE
			      C 					CopyFrom Timer_UserBuild, 0
			      C 				.ENDIF
			      C 			.ENDIF
			      C 		POPA
